{
  "name": "codespaces-for-webflow",
  "version": "0.0.0",
  "description": "Minimalistic template repository for building rich native JavaScript / jQuery applications for Webflow",
  "default": "src/index.html",
  "scripts": {
    "clean": "rimraf dist",
    "start": "parcel src/index.html src/pages/cleaningProposal.html",
    "build": "parcel build src/index.html src/pages/cleaningProposal.html --dist-dir ./dist --no-source-maps --no-hmr"
  },
  "author": "Leon B. Barth",
  "license": "MIT",
  "devDependencies": {
    "parcel": "^2.8.3",
    "process": "^0.11.10",
    "typescript": "^5.0.4",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@chenfengyuan/datepicker": "^1.0.10",
    "core-js": "^3.29.0",
    "jquery": "^3.7.1",
    "regenerator-runtime": "^0.13.11"
  }
}
